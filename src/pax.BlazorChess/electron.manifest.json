{
  "executable": "pax.BlazorChess",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "pax.BlazorChess",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "win": {
      "publish": [ "github" ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./bin/wwwroot/images/icon-512x512.png"
    },    
    "appId": "com.pax.BlazorChess.app",
    "productName": "pax.BlazorChess",
    "copyright": "Copyright Â© 2022",
    "buildVersion": "0.6.2",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  },
  "user": "Ipax77",
  "project": "pax.BlazorChess",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipax77/pax.BlazorChess.git"
  }  
}